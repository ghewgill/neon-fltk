DECLARE EXTENSION FUNCTION MenuBar_copy(m: MenuBar, items: Array<MenuItem>)
DECLARE EXTENSION FUNCTION TextBuffer_addModifyCallback(b: TextBuffer, cb: TextBufferModifyCallback)
DECLARE EXTENSION FUNCTION TextBuffer_searchForward(b: TextBuffer, startPos: Number, searchString: String, OUT foundPos: Number): Boolean

FUNCTION MenuBar.copy(self: MenuBar, items: Array<MenuItem>)
    MenuBar_copy(self, items)
END FUNCTION

FUNCTION TextBuffer.addModifyCallback(b: TextBuffer, cb: TextBufferModifyCallback)
    TextBuffer_addModifyCallback(b, cb)
END FUNCTION

FUNCTION TextBuffer.searchForward(self: TextBuffer, startPos: Number, searchString: String, OUT foundPos: Number): Boolean
    RETURN TextBuffer_searchForward(self, startPos, searchString, OUT foundPos)
END FUNCTION

